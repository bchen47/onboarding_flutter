{
    "name": "bestlife",
    "version": "1.0.0",
    "private": true,
    "scripts": {
      "start": "rm -rf ./assets/www && webpack --config webpack.config.js --progress --watch --env.development",
      "build:dev": "rm -rf ./assets/www && webpack --config webpack.config.js",
      "build:prod": "rm -rf ./assets/www && webpack --config webpack.config.js -p --progress --env.production",
